version 16
__lua__
-- langtons ant ğŸ±
-- by wahtzy ğŸ˜

res = 1
cols = 128 / res
rows = 128 / res

white = 6
black = 1
ant = 8

ant_x = flr(cols/2)
ant_y = flr(rows/2)
ant_d = 0

ca = {}

function _init()
	ca = new_array(cols,rows)
end

function _update()
	ant_move()
end

function _draw()
	draw_array(ca)
	ant_draw()
end

-->8
-- cell control
function draw_cell(x,y,c)
	x1 = x * res
	y1 = y * res
	
	rectfill(x1,y1,x1+res-1,y1+res-1,c)
end

function get_cell(x,y)
	return pget(x * res, y * res)
end

function new_array(x,y)
	a = {}
	for i=0,x do
		a[i] = {}
		for j=0,y do
			a[i][j] = black
		end
	end
	return a
end

function draw_array(a)
	for x=0,cols do
		for y=0,rows do
			draw_cell(x,y,a[x][y])
		end
	end
end


-->8
-- ant control

function ant_draw()
	draw_cell(ant_x,ant_y,ant)
end

function ant_move()
	if a[ant_x][ant_y] == white 
	then
		ant_d -= 1
		ant_d = (ant_d + 4) % 4
		a[ant_x][ant_y] = black
	elseif a[ant_x][ant_y] == black
	then
		ant_d += 1
		ant_d = (ant_d + 4) % 4
		a[ant_x][ant_y] = white
	end
	
	if (ant_d == 0) ant_y -= 1
	if (ant_d == 1) ant_x += 1
	if (ant_d == 2)	ant_y += 1
	if (ant_d == 3)	ant_x -= 1
	
	ant_x = (ant_x + cols) % cols
	ant_y = (ant_y + rows) % rows
	
end
